<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.caterbao.lumos.locals.dal.mapper.BookBorrowFlowLogMapper">
    <select id="find" resultType="com.caterbao.lumos.locals.dal.pojo.BookBorrowFlowLog" parameterType="com.caterbao.lumos.locals.dal.LumosSelective">
        select ${fields} from BookBorrowFlowLog where 1=1
        <if test="where.MerchId != null">and MerchId =#{where.MerchId}</if>
        <if test="where.FlowId != null">and FlowId =#{where.FlowId}</if>
        <if test="where.TrgId != null">and TrgId =#{where.TrgId}</if>
        <if test="where.DeviceId != null">and DeviceId =#{where.DeviceId}</if>
    </select>
    <insert id="insert" parameterType="com.caterbao.lumos.locals.dal.pojo.BookBorrowFlowLog">
  insert into BookBorrowFlowLog(id,trgId,MerchId,deviceId,deviceCode,flowId,actionCode,actionName,actionData,actionResult,actionRemark, actionTime,creator,createTime)
  values(#{id},#{trgId},#{merchId},#{deviceId},#{deviceCode},#{flowId},#{actionCode},#{actionName},#{actionData},#{actionResult},#{actionRemark},#{actionTime},#{creator},#{createTime})
    </insert>
</mapper>
