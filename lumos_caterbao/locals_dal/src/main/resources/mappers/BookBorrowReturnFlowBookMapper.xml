<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.caterbao.lumos.locals.dal.mapper.BookBorrowReturnFlowBookMapper">
    <select id="findOne" resultType="com.caterbao.lumos.locals.dal.pojo.BookBorrowReturnFlowBook" parameterType="com.caterbao.lumos.locals.dal.LumosSelective">
        select  ${fields} from BookBorrowReturnFlowBook where 1=1
        <if test="where.FlowId != null">and FlowId =#{where.FlowId}</if>
        <if test="where.SkuRfId != null">and SkuRfId =#{where.SkuRfId}</if>
    </select>
    <insert id="insert" parameterType="com.caterbao.lumos.locals.dal.pojo.BookBorrowReturnFlowBook">
  insert into BookBorrowReturnFlowBook(id,flowId,clientUserId,skuId,skuName,skuImgUrl,skuCumCode, skuRfId,borrowTime,creator,createTime)
  values(#{id},#{flowId},#{clientUserId},#{skuId},#{skuName},#{skuImgUrl},#{skuCumCode},#{skuRfId},#{borrowTime},#{creator},#{createTime})
    </insert>
</mapper>
