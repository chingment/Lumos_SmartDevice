<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.caterbao.lumos.locals.dal.mapper.DeviceCabinetMapper">

    <select id="findByDeviceId" resultType="com.caterbao.lumos.locals.dal.pojo.DeviceCabinet">
        select * from DeviceCabinet
        where DeviceId=#{deviceId}
    </select>

</mapper>
